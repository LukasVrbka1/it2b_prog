#include <stdio.h>
#include <stdlib.h>
#include <time.h>

int main() {
    
    int pole[] = {10, 20, 30, 40, 50};
    int velikost = (pole) / (pole[0]);

    
    srand(time(NULL));

   
    int index = rand() % velikost;

    
    printf("Náhodný prvek z pole: %d\n", pole[index]);

    return 0;
}
