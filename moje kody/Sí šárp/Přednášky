class Program
{
    static void Main()
    {
        Console.Write("Zadej počet přednášek: ");
        int pocetPrednasek = int.Parse(Console.ReadLine());

        Console.Write("Zadej počet seminářů: ");
        int pocetSeminaru = int.Parse(Console.ReadLine());

        Console.Write("zadej pocet hodin");
        int hodinovaMzda = int.Parse(Console.ReadLine());

        int celkoveHodiny = pocetPrednasek * 2 + pocetSeminaru;
        int vyplata = celkoveHodiny * hodinovaMzda;

        Console.WriteLine("Celková výplata profesora je: " + vyplata + " Kč");






    }
}



